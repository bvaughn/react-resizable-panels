        {
          "content": "<p>Default layout for the Group.</p>\n"
        },
        {
          "content": "<p>This value allows layouts to be remembered between page reloads.</p>\n",
          "intent": "primary"
        },
        {
          "content": "<p>Refer to the documentation for how to avoid layout shift when using server components.</p>\n",
          "intent": "warning"
        }
