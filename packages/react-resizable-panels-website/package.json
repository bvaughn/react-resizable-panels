{
  "name": "react-resizable-panels-website",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "parcel build \"index.html\"",
    "clear": "pnpm run clear:parcel-cache & pnpm run clear:node_modules",
    "clear:node_modules": "rm -rf ./node_modules",
    "clear:parcel-cache": "rm -rf ./.parcel-cache",
    "kill-port": "kill-port ${PORT:-1234}",
    "test:e2e": "playwright test",
    "test:e2e:debug": "DEBUG=true playwright test",
    "watch": "parcel \"index.html\""
  },
  "dependencies": {
    "@codemirror/lang-css": "latest",
    "@codemirror/lang-html": "latest",
    "@codemirror/lang-javascript": "latest",
    "@codemirror/lang-markdown": "latest",
    "@codemirror/language": "latest",
    "@codemirror/state": "latest",
    "@lezer/highlight": "latest",
    "kill-port": "latest",
    "localforage": "latest",
    "match-sorter": "latest",
    "react": "experimental",
    "react-dom": "experimental",
    "react-resizable-panels": "*",
    "react-router-dom": "latest",
    "react-virtualized-auto-sizer": "^1.0.10",
    "sort-by": "latest",
    "suspense": "^0.0.38"
  }
}
